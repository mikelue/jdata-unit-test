<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xsi:schemaLocation="
		http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd
		http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
	">
	<changeSet id="operate" labels="operate" author="${testClassName}">
		<createTable tableName="tab_did">
			<column name="ti_id" type="INTEGER" autoIncrement="true">
				<constraints primaryKey="true" primaryKeyName="pk_tab_did" />
			</column>
			<column name="ti_value" type="VARCHAR(32)">
				<constraints nullable="false" />
			</column>
		</createTable>

		<rollback>
			<dropTable tableName="tab_did" />
		</rollback>
	</changeSet>
</databaseChangeLog>
